package ads.l2.adaptivecruisecontrol;

import ADS.interfaces.IL2_ACC;

public class ACC implements IL2_ACC {
	
	protected boolean isStarted = false;
	
	@Override
	public void start() {
		// TODO Auto-generated method stub
		isStarted = true;
		
		
	}

	@Override
	public void stop() {
		// TODO Auto-generated method stub
		isStarted = false;
	}

	@Override
	public boolean isStarted() {
		// TODO Auto-generated method stub
		return isStarted;
	}

}
